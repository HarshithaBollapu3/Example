package Threadapp;

public class Threaddemo extends Thread {
	 public void run(){
		 for(int i=0;i<10;i++){
			try{
				Thread.sleep(3000);
			}
			catch(Exception e){
				System.out.println(e);
			}
			System.out.println(e);
		 }
	 }

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        Threaddemo obj=new Threaddemo();
        obj.start();
	}

}
